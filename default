server {                                                                                                                                                                                                                   listen 80 default_server;                                                                                                                                                                                          listen [::]:80 default_server;                                                                                                                                                                                                                                                                                                                                                                                                        root /var/www/html;                                                                                                                                                                                                index index.html index.htm index.nginx-debian.html;                                                                                                                                                                                                                                                                                                                                                                                   server_name 196.189.126.37;                                                                                                                                                                                                                                                                                                                                                                                                           location / {                                                                                 